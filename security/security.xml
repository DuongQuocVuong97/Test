<odoo>

    <record id="group_manager" model="res.groups">
        <field name="name">CRM Customer Request / Manager</field>
    </record>

    <record id="group_crm_lead_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_crm_customer_request"/>
        <field name="implied_ids" eval="[(4,ref('crm.group_user'))]"/>
    </record>

    <record id="group_crm_lead_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_crm_customer_request"/>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
    </record>


</odoo>